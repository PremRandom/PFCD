   ],
   "source": [
    "if __name__ == \"__main__\":\n",
    "    num = int(input(\"Enter no. to get sum of digits: \"))\n",
    "    print(f\"Sum of digits of {num} is: {sum_of_digits(num)}\")"
   ]
